<h1>Sign Up</h1>

 <%= form_for @user do |f| %>
  <div class = "field">
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div class= "field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class= "field">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <br>
  <%= f.submit %>
<% end %>

<p><%= link_to 'Sign Up With Google', '/auth/google_oauth2' %></p>
