<h1><%= @resource.full_title %></h1>
<%= link_to "Edit Resource", edit_resource_path(@resource) %>
<p>Created at: <%= @resource.created_at %></p>

<% if @resource.car_id %>
<p>  Car: <%=link_to @resource.car.full_title, car_path(@resource.car) %></p>
<% end %>

<p><%= link_to "Go To Resource", @resource.source_url %></p>
